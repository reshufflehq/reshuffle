{
  "name": "reshuffle",
  "description": "Reshuffle CLI Tool",
  "version": "0.7.1",
  "author": "arikmaor @arikmaor",
  "bin": {
    "reshuffle": "./bin/run"
  },
  "bugs": "https://github.com/reshufflehq/reshuffle/issues",
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/reshufflehq/reshuffle.git",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "reshuffle",
    "repositoryPrefix": "<%- repo %>/blob/cli_v<%- version %>/cli/<%- commandPath %>",
    "plugins": [
      "@oclif/plugin-help",
      "@binaris/oclif-plugin-not-found"
    ]
  },
  "repository": "reshufflehq/reshuffle",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest",
    "lint": "eslint -c ../common/.eslintrc.json --ext .ts src",
    "test": "ava lib/test/**.spec.js lib/test/**.test.js",
    "test:update": "ava -u lib/test/**.spec.js lib/test/**.test.js",
    "build": "npm run prepack"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@binaris/oclif-plugin-not-found": "^1.2.4",
    "@binaris/spice-node-client": "^2.2.0",
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/errors": "^1.2.2",
    "@oclif/parser": "^3.8.4",
    "@oclif/plugin-help": "^2.2.1",
    "@reshuffle/utils-subprocess": "0.0.2",
    "any-shell-escape": "^0.1.1",
    "chalk": "^2.4.2",
    "cli-ux": "^5.3.3",
    "columnify": "^1.5.4",
    "conf": "^6.2.0",
    "dedent": "^0.7.0",
    "dotenv": "^8.2.0",
    "find-up": "^4.1.0",
    "fs-extra": "^8.1.0",
    "js-yaml": "^3.13.1",
    "lodash.has": "^4.5.2",
    "lodash.memoize": "^4.1.2",
    "ms": "^2.1.2",
    "mz": "^2.7.0",
    "node-fetch": "^2.6.0",
    "open": "^7.0.0",
    "prompts": "^2.3.0",
    "tar": "^5.0.5",
    "terminal-link": "^2.0.0",
    "tslib": "^1.10.0",
    "validator": "^12.1.0"
  },
  "devDependencies": {
    "@binaris/spice-koa-server": "^2.2.0",
    "@oclif/dev-cli": "^1.22.2",
    "@types/dedent": "^0.7.0",
    "@types/fs-extra": "^8.0.1",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash.has": "^4.5.6",
    "@types/lodash.memoize": "^4.1.6",
    "@types/ms": "^0.7.31",
    "@types/mz": "^2.7.0",
    "@types/node": "^12.12.14",
    "@types/node-fetch": "^2.5.3",
    "@types/prompts": "^2.0.3",
    "@types/tar": "^4.0.3",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "ava": "^2.4.0",
    "eslint": "^6.7.2",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-no-only-tests": "^2.3.1",
    "globby": "^10.0.1",
    "specshell": "^0.0.3",
    "testdouble": "^3.12.4",
    "ts-node": "^8.5.0",
    "typescript": "^3.7.2"
  }
}
